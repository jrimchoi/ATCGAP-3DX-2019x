      <CATCmdStarter Name = "Zoom" Command = "Zoom" />
      <CATCmdStarter Name = "Pan" Command = "Pan" />
      <CATCmdStarter Name = "Reframe" Command = "Reframe" />
      <CATCmdStarter Name = "Previous" Command = "Previous" />
      <CATCmdStarter Name = "Next" Command = "Next" />

    </CATCmdWorkbench>
