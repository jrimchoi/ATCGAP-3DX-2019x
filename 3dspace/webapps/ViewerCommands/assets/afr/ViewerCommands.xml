  <Template syp:name="3DShareAfrWorkshopAccess" Target="CATCmdWorkbench">
    <CATCmdWorkbench Name="3DSHAREWorkbench">
      <CATCmdContainer Name = "Views" Visibility = "1" RepresentationType="AfrActionBarSection">
        <CATCmdStarter Name = "Reframe"       Command = "VisuReframeView" />
        <CATCmdStarter Name = "Pan"           Command = "VisuPanView" />
        <CATCmdStarter Name = "Zoom"          Command = "VisuZoomView" />
