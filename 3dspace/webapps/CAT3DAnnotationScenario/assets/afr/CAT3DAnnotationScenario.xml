<?xml version="1.0" encoding="utf-8" ?>

<Styles xmlns:syp="http://www.3ds.com/xsd/SYP">

  <Template syp:name="CAT3DAnnotationPlayExperienceHeaders" Target="CATCmdHeadersList">
    <CATCmdHeadersList>
      <CATCommandHeader ID="CATWebUXSlideShowHdr" ClassName="DS/CATWebUXSlideShow/CATWebUXSlideShowCmd" ResourceFile="CATWebUXSlideShow/CATWebUXSlideShow" HelpResourceFile="CATWebUXSlideShow/CATWebUXSlideShow" Behavior="CATAfrHeaderCheck">
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg ID="ControllerID" Value="CAT3DAnnotationSlideShow" Mode="In" />
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>
      
      <CATCommandHeader ID="CAT3DAnnotationResetDisplayHistoryHdr"  ClassName="DS/CAT3DAnnotationBaseCmds/CAT3DAnnotationResetDisplayHistoryCmd"      ResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds"/>
      <CATCommandHeader ID="CAT3DAnnotationPreferencesHdr"          ClassName="DS/CAT3DAnnotationBaseCmds/CAT3DAnnotationPreferencesCmd"              ResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds" HelpResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds"/>

      <CATCommandHeader ID="CAT3DAnnotationLevelSelectorHdr"        ClassName="DS/CAT3DAnnotationBaseCmds/CAT3DAnnotationLevelSelectorCmd"            ResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds" HelpResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds"/>
      <CATCommandHeader ID="CAT3DAnnotationHideHdr"                 ClassName="DS/CAT3DAnnotationBaseCmds/CAT3DAnnotationShowHideCmd"                 ResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds" HelpResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds"/>
      <CATCommandHeader ID="CAT3DAnnotationShowHdr"                 ClassName="DS/CAT3DAnnotationBaseCmds/CAT3DAnnotationShowHideCmd"                 ResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds" HelpResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds"/>
      <CATCommandHeader ID="CAT3DAnnotationSetHideHdr"              ClassName="DS/CAT3DAnnotationBaseCmds/CAT3DAnnotationShowHideCmd"                 ResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds" HelpResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds"/>
      <CATCommandHeader ID="CAT3DAnnotationSetShowHdr"              ClassName="DS/CAT3DAnnotationBaseCmds/CAT3DAnnotationShowHideCmd"                 ResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds" HelpResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds"/>
      
      <CATCommandHeader ID = "Zoom"     ClassName = "DS/ViewerCommands/VisuZoomViewCmd"     HelpResourceFile = "3DPlay/3DPlayHelpForCommands3D" ResourceFile = "3DPlay/3DPlayExperience3D" />
      <CATCommandHeader ID = "Pan"      ClassName = "DS/ViewerCommands/VisuPanViewCmd"      HelpResourceFile = "3DPlay/3DPlayHelpForCommands3D" ResourceFile = "3DPlay/3DPlayExperience3D" />
      <CATCommandHeader ID = "Rotate"   ClassName = "DS/ViewerCommands/VisuRotateViewCmd"   HelpResourceFile = "3DPlay/3DPlayHelpForCommands3D" ResourceFile = "3DPlay/3DPlayExperience3D" />
      <CATCommandHeader ID = "Reframe"  ClassName = "DS/ViewerCommands/VisuReframeViewCmd"  HelpResourceFile = "3DPlay/3DPlayHelpForCommands3D" ResourceFile = "3DPlay/3DPlayExperience3D" />
      <CATCommandHeader ID = "VisuOrthographicView"  ClassName = "DS/ViewerCommands/VisuOrthographicViewCmd" SharedLibraryName = "ShareUI" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" ResourceFile = "ViewerCommands/ViewerCommands" />
      <CATCommandHeader ID = "VisuPerspectiveView"  ClassName = "DS/ViewerCommands/VisuPerspectiveViewCmd" SharedLibraryName = "ShareUI" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" ResourceFile = "ViewerCommands/ViewerCommands" />
      <CATCommandHeader ID = "ViewSelector" ClassName = "DS/3DPlayCommands/CmdViewSelector" HelpResourceFile = "3DPlay/3DPlayHelpForCommands3D" ResourceFile = "3DPlay/3DPlayExperience3D" />
      <CATCommandHeader ID = "VisuNoShadingEdges" ClassName = "DS/ViewerCommands/VisuNoShadingEdgesCmd" SharedLibraryName = "ShareUI" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" ResourceFile = "ViewerCommands/ViewerCommands" />
      <CATCommandHeader ID = "VisuShading" ClassName = "DS/ViewerCommands/VisuShadingCmd" SharedLibraryName = "ShareUI" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" ResourceFile = "ViewerCommands/ViewerCommands" />
      <CATCommandHeader ID = "VisuShadingEdges" ClassName = "DS/ViewerCommands/VisuShadingEdgesCmd" SharedLibraryName = "ShareUI" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" ResourceFile = "ViewerCommands/ViewerCommands" />
      <CATCommandHeader ID = "VisuShadingMaterial" ClassName = "DS/ViewerCommands/VisuShadingMaterialCmd" SharedLibraryName = "ShareUI" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" ResourceFile = "ViewerCommands/ViewerCommands" />
      <CATCommandHeader ID = "VisuShadingMaterialEdges" ClassName = "DS/ViewerCommands/VisuShadingMaterialEdgesCmd" SharedLibraryName = "ShareUI" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" ResourceFile = "ViewerCommands/ViewerCommands" />
      <CATCommandHeader ID = "VisuNoShadingEdges" ClassName = "DS/ViewerCommands/VisuNoShadingEdgesCmd" SharedLibraryName = "ShareUI" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" ResourceFile = "ViewerCommands/ViewerCommands" />
      <CATCommandHeader ID = "VisuShadingEdgesHiddenEdges" ClassName = "DS/ViewerCommands/VisuShadingEdgesHiddenEdgesCmd" SharedLibraryName = "ShareUI" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" ResourceFile = "ViewerCommands/ViewerCommands" />
      <CATCommandHeader ID = "VisuShadingEdgesNoSmoothEdges"  ClassName = "DS/ViewerCommands/VisuShadingEdgesNoSmoothEdgesCmd" SharedLibraryName = "ShareUI" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" ResourceFile = "ViewerCommands/ViewerCommands" />
      <CATCommandHeader ID = "VisuShadingIllustration" ClassName = "DS/ViewerCommands/VisuShadingIllustrationCmd" SharedLibraryName = "ShareUI" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" ResourceFile = "ViewerCommands/ViewerCommands" />
      
      <CATCommandHeader ID = "VisibilityCommands" ClassName = "DS/3DPlayCommands/VisibilityCommands" HelpResourceFile = "3DPlay/3DPlayHelpForCommands3D" ResourceFile = "3DPlay/3DPlayExperience3D" />
      <CATCommandHeader ID = "AnnotationCommands" ClassName = "DS/3DPlayAnnotation3D/AnnotationCommands" HelpResourceFile = "3DPlay/3DPlayHelpForCommands3D" ResourceFile = "3DPlay/3DPlayExperience3D" />
      <CATCommandHeader ID = "AnnotationTour" ClassName = "DS/3DPlayAnnotation3D/AnnotationTour" HelpResourceFile = "3DPlay/3DPlayHelpForCommands3D" ResourceFile = "3DPlay/3DPlayExperience3D" />
      <CATCommandHeader ID = "LineicFilter" ClassName = "DS/3DPlayCommands/CmdLineicFilter" HelpResourceFile = "3DPlay/3DPlayHelpForCommands3D" ResourceFile = "3DPlay/3DPlayExperience3D" />

      <CATCommandHeader ID ="measure2CmdHdr" ClassName="DS/DMUMeasure/DMUCreateMeasureCmd" ResourceFile="DMUBaseCommands/3DPlayPro" HelpResourceFile="DMUMeasure/DMUMeasure">
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg ID = "3dPlay" Value = "3dPlay" Mode = "In" />
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>
    </CATCmdHeadersList>
  </Template>

  <Template syp:name="CAT3DAnnotationPlayExperienceAccess" Target="CATCmdWorkbench">
    <CATCmdWorkbench Name="CAT3DAnnotationPlayExperienceWorkbench">

      <CATCmdContainer Name="Fixed Area" RepresentationType="AfrActionBarFixedSection">
        <CATCmdStarter Name="CATWebUXSlideShowStr" Command="CATWebUXSlideShowHdr" />
        <CATCmdSeparator Name = "VisuMode_Separator"/>
      </CATCmdContainer>
      
      <CATCmdContainer Name = "SectionViews" Visibility = "1" RepresentationType="AfrActionBarSection">
        <CATCmdStarter Name = "Rotate"        Command = "Rotate"  Responsive="Long Large"/>
        <CATCmdStarter Name = "Pan"           Command = "Pan"     Responsive="Long Large"/>
        <CATCmdStarter Name = "Zoom"          Command = "Zoom"    Responsive="Long Large"/>
        <CATCmdStarter Name = "Reframe"       Command = "Reframe" Responsive="Long Large"/>
        <CATCmdSeparator Name = "VisuMode_Separator"/>
        <CATCmdStarter Name = "ViewSelector"          Command = "ViewSelector" Responsive="Small Medium Long Large"/>
        <CATCmdContainer Name = "CameraViews" Visibility = "1" RepresentationType="AfrActionBarDropDown" Responsive="Medium Long Large">
          <CATCmdStarter Name = "OrthoView"  Command = "VisuOrthographicView" />
          <CATCmdStarter Name = "PerspView"  Command = "VisuPerspectiveView" />
        </CATCmdContainer>

        <CATCmdContainer Name = "Rendering" Visibility = "1" RepresentationType="AfrActionBarDropDown" Responsive="Medium Long Large">
          <CATCmdStarter Name = "ShadingEdges"  Command = "VisuShadingEdges" />
          <CATCmdStarter Name = "Shading"  Command = "VisuShading" />
          <CATCmdStarter Name = "ShadingEdgesNoSmoothEdges"  Command = "VisuShadingEdgesNoSmoothEdges" />
          <CATCmdStarter Name = "ShadingEdgesHiddenEdges"  Command = "VisuShadingEdgesHiddenEdges" />
          <CATCmdStarter Name = "ShadingMaterialEdges"  Command = "VisuShadingMaterialEdges" />
          <CATCmdStarter Name = "ShadingMaterial"  Command = "VisuShadingMaterial" />
          <CATCmdStarter Name = "VisuShadingIllustration"  Command = "VisuShadingIllustration" />
          <CATCmdStarter Name = "Wireframe"  Command = "VisuNoShadingEdges" />
        </CATCmdContainer>
        <CATCmdStarter Name = "LineicFilter"  Command = "LineicFilter" Responsive="Medium Long Large"/>
      </CATCmdContainer>

      <CATCmdContainer Name = "SectionTools" Visibility = "1" RepresentationType="AfrActionBarSection">
        <CATCmdStarter Name = "VisibilityCommands"  Command = "VisibilityCommands" Responsive="Medium Long Large"/>
        <CATCmdStarter Name = "Measure"  Command = "measure2CmdHdr"  Responsive="Medium Long Large"/>
        <CATCmdStarter Name = "AnnotationCommands"  Command = "AnnotationCommands"  Responsive="Medium Long Large"/>
        <CATCmdStarter Name = "AnnotationTour"  Command = "AnnotationTour"  Responsive="Long Large"/>
        <CATCmdSeparator Name = "VisuMode_Separator"/>
        <CATCmdStarter Name = "CAT3DAnnotationPreferencesStr" Command = "CAT3DAnnotationPreferencesHdr"/>
      </CATCmdContainer>
      
    </CATCmdWorkbench>
  </Template>
</Styles>
