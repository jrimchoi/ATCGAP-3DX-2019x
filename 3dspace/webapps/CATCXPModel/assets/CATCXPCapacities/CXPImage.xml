<?xml version="1.0" encoding="UTF-8"?>
<IdeaExperienceCapacities>
  <Header>
    <Version>0.1</Version>
  </Header>

  <ExperienceObject>

    <CXPEvent id="UIClicked">
      <NaturalLanguageInfo capacityType="SensorEvent" alias="Is Clicked" icon="icons/caps/I_CXPClick.png"/>
    </CXPEvent>

    <CXPEvent id="UIDoubleClicked">
      <NaturalLanguageInfo capacityType="SensorEvent" alias="Is Double Clicked" icon="icons/caps/I_CXPDoubleClick.png"/>
    </CXPEvent>
    
    <CXPFunction id="setImage" body="setImage" returnValue="Boolean">
      <Variable name="image" type="Object" restrictiveTypes="CXPResource_Spec"/>
      <NaturalLanguageInfo capacityType="DriverState" alias="sets image" icon="icons/20px/I_CXPSetsImage.png" exposedVariable="image"/>
    </CXPFunction>

  </ExperienceObject>

</IdeaExperienceCapacities>
