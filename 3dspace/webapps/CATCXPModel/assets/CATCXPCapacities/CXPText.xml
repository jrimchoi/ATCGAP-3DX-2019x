<?xml version="1.0" encoding="UTF-8"?>
<IdeaExperienceCapacities>
  <Header>
    <Version>0.1</Version>
  </Header>

  <ExperienceObject>

    <CXPEvent id="UIClicked">
      <NaturalLanguageInfo capacityType="SensorEvent" alias="Is Clicked" icon="icons/caps/I_CXPClick.png"/>
    </CXPEvent>

    <CXPEvent id="UIDoubleClicked">
      <NaturalLanguageInfo capacityType="SensorEvent" alias="Is Double Clicked" icon="icons/caps/I_CXPDoubleClick.png"/>
    </CXPEvent>

    <CXPFunction id="setText" body="setText" returnValue="Boolean">
      <Variable name="text" type="String"/>
      <NaturalLanguageInfo capacityType="DriverState" alias="sets text" icon="icons/20px/I_CXPDisplayText.png" exposedVariable="text"/>
    </CXPFunction>

  </ExperienceObject>

</IdeaExperienceCapacities>
