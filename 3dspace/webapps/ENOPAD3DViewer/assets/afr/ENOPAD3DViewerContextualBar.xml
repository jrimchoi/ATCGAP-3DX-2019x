<?xml version="1.0" encoding="utf-8" ?>
<Styles xmlns:syp="http://www.3ds.com/xsd/SYP">

      <!-- Command headers -->
      <Template syp:name="ENOPAD3DViewerContextBarCommandHeaders" Target="CATCmdHeadersList">
            <CATCmdHeadersList>
                  <!-- ClassName and ResourceFile attributes handle AMD path, e.g.  MyModule/MyFileWithoutExtension -->
                  <CATCommandHeader ID="LevelSelectorHdr" ClassName="DS/ENOPAD3DViewer/commands/PAD3DViewerLevelSelectorCmd" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer" Available="1"/>
                  <CATCommandHeader ID="RemoveRootHdr" ClassName="DS/ENOPAD3DViewer/commands/PAD3DViewerRemoveRootCmd" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer" Available="1"/>
                  <CATCommandHeader ID="ReframeOnHdr" ClassName="DS/ENOPAD3DViewer/commands/PAD3DViewerReframeOnCmd" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer" Available="1"/>
                  <CATCommandHeader ID="CenterTreeHdr" ClassName="DS/ENOPAD3DViewer/commands/PAD3DViewerCenterTreeCmd" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer" Available="1"/>
                  <CATCommandHeader ID="FocusOnHdr" ClassName="DS/ENOPAD3DViewer/commands/PAD3DViewerFocusOnCmd" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer" Available="1"/>
                  <CATCommandHeader ID="HideShowHdr" ClassName="DS/ENOPAD3DViewer/commands/PAD3DViewerHideShowCmd" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer"/>
                  <CATCommandHeader ID="SwitchVisuOnDemandHdr" ClassName="DS/ENOPAD3DViewer/commands/PAD3DViewerSwitchVisuOnDemandCmd"  ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer"/>
                  <CATCommandHeader ID="ActionBar_Attributes" ClassName="DS/ENOPAD3DViewer/commands/PAD3DViewerEditPropertiesCmd" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer" Available="1"/>

                  <!-- Ambiances -->
                  <CATCommandHeader ID="VisuV6Env" ClassName="DS/ViewerCommands/VisuV6EnvCmd" ResourceFile="ViewerCommands/ViewerCommands"/>
                  <CATCommandHeader ID="VisuNoEnv" ClassName="DS/ViewerCommands/VisuNoEnvCmd" ResourceFile="ViewerCommands/ViewerCommands"/>
                  <CATCommandHeader ID="VisuCleanSpaceEnv" ClassName="DS/ViewerCommands/VisuCleanSpaceEnvCmd" ResourceFile="ViewerCommands/ViewerCommands"/>
                  <CATCommandHeader ID="VisuDarkBlueEnv" ClassName="DS/ViewerCommands/VisuDarkBlueEnvCmd" ResourceFile="ViewerCommands/ViewerCommands"/>
                  <CATCommandHeader ID="VisuDarkGreyEnv" ClassName="DS/ViewerCommands/VisuDarkGreyEnvCmd" ResourceFile="ViewerCommands/ViewerCommands"/>
                  <CATCommandHeader ID="VisuShinyEnv" ClassName="DS/ViewerCommands/VisuShinyEnvCmd" ResourceFile="ViewerCommands/ViewerCommands"/>
            </CATCmdHeadersList>
      </Template>
</Styles>
