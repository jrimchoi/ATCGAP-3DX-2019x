<?xml version="1.0" encoding="utf-8" ?>
<Styles xmlns:syp="http://www.3ds.com/xsd/SYP">
    <Template syp:name="SnapshotsHeaders" Target="CATCmdHeadersList">
        <CATCmdHeadersList>
            <CATCommandHeader ID="SnapshotDetailsHdr" ClassName="DS/ListSnapshots/Cmd" ResourceFile="ListSnapshots/Cmd" Available="1" />
            <CATCommandHeader ID="CompareJSONHdr" ClassName="DS/CompareCmd/CompareCmd" ResourceFile="CompareCmd/CompareCmd" Available="1" />
            <CATCommandHeader ID="DeleteSnapshotsHdr" ClassName="DS/ListSnapshots/Cmd" ResourceFile="ListSnapshots/Cmd" Available="1" />
            <CATCommandHeader ID="RestoreSnapshotHdr" ClassName="DS/ListSnapshots/Cmd" ResourceFile="ListSnapshots/Cmd" Available="1" />
            <CATCommandHeader ID="OpenInCatiaHdr" ClassName="DS/ListSnapshots/Cmd" ResourceFile="ListSnapshots/Cmd" Available="1" />
            <CATCommandHeader ID="CustomizeColsHdr" ClassName="DS/ListSnapshots/Cmd" ResourceFile="ListSnapshots/Cmd" Available="1" />            
        </CATCmdHeadersList>
    </Template>

    <Template syp:name="SnapshotsWkbAccess" Target="CATCmdWorkbench">
        <CATCmdWorkbench Name="SnapshotsWkb">
            <CATCmdContainer Name="Snapshots" RepresentationType="AfrActionBarSection">
                <CATCmdStarter Name="Details" Command="SnapshotDetailsHdr" />
                <CATCmdStarter Name="CompareJSON" Command="CompareJSONHdr" />
                <CATCmdStarter Name="Delete" Command="DeleteSnapshotsHdr" />
                <CATCmdStarter Name="Restore" Command="RestoreSnapshotHdr" />
                <CATCmdStarter Name="OpenInCatia" Command="OpenInCatiaHdr" />
                <CATCmdStarter Name="CustomizeCols" Command="CustomizeColsHdr" />
            </CATCmdContainer>
        </CATCmdWorkbench>
    </Template>
</Styles>
