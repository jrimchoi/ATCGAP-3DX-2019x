    <vplm.Package name="EmbeddedCommunications" />
    <m1.package name="EmbeddedCommunications" />

    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMCoreRepReference name="EEMatrixRepresentation" />
      <m1.type name="EEMatrixRepresentation" />

    </pattern.VPLM_CLASS>

    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMCoreReference name="EEMatrix" />
      <m1.type name="EEMatrix" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="protocol" />
        <m1.attribute name="EEMatrix.protocol" local="yes" />
      </pattern.VPLM_SIMPLE>

    </pattern.VPLM_CLASS>

    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMCoreReference name="EESystemSignal" />
      <m1.type name="EESystemSignal" />

    </pattern.VPLM_CLASS>

  </pattern.VPLM_PACKAGE>

</mapping>
