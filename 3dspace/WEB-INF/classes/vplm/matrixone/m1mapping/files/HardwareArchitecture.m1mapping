      <m1.type name="EEGenericConnectorType" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="subTechnology" />
        <m1.attribute name="EEGenericConnectorType.subTechnology" local="yes" />
      </pattern.VPLM_SIMPLE>

    </pattern.VPLM_CLASS>

    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMCoreReference name="EESubConnectorType" />
      <m1.type name="EESubConnectorType" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="subTechnology" />
        <m1.attribute name="EESubConnectorType.subTechnology" local="yes" />
      </pattern.VPLM_SIMPLE>

    </pattern.VPLM_CLASS>

    <!-- RelationClass -->
    <pattern.VPLM_RELATIONCLASS>
      <vplm.PLMCoreInstance name="EECommunicationConnector" />
      <m1.relationship name="EECommunicationConnector" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="flexRayLatestTx" />
