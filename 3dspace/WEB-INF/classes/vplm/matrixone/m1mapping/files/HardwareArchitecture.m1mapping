<?xml version="1.0" encoding="UTF-8"?>
<mapping xmlns="http://www.3ds.com/enovia-matrixone/vplm-m1/mapping" version="7">

  <!-- Package -->
  <pattern.VPLM_PACKAGE>
    <vplm.Package name="HardwareArchitecture" />
    <m1.package name="HardwareArchitecture" />

    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMConnection name="EESRChannelCluster" />
      <m1.type name="EESRChannelCluster" />

    </pattern.VPLM_CLASS>

    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMConnection name="EESRConnectorBus1" />
      <m1.type name="EESRConnectorBus1" />

    </pattern.VPLM_CLASS>

    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMConnection name="EESRConnectorBus" />
      <m1.type name="EESRConnectorBus" />

    </pattern.VPLM_CLASS>

    <!-- RelationClass -->
    <pattern.VPLM_RELATIONCLASS>
      <vplm.PLMCoreInstance name="EEECUInstance" />
      <m1.relationship name="EEECUInstance" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="comConfigurationGwTimeBase" />
        <m1.attribute name="EEECUInstance.comConfigurationGwTimeBase" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="comConfigurationRxTimeBase" />
        <m1.attribute name="EEECUInstance.comConfigurationRxTimeBase" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="comConfigurationTxTimeBase" />
        <m1.attribute name="EEECUInstance.comConfigurationTxTimeBase" local="yes" />
      </pattern.VPLM_SIMPLE>

    </pattern.VPLM_RELATIONCLASS>

    <!-- RelationClass -->
    <pattern.VPLM_RELATIONCLASS>
      <vplm.PLMCoreInstance name="EESubSystem" />
      <m1.relationship name="EESubSystem" />

    </pattern.VPLM_RELATIONCLASS>

    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMCoreReference name="EEConnectorType" />
      <m1.type name="EEConnectorType" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="linkTechnology" />
        <m1.attribute name="EEConnectorType.linkTechnology" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
