        <m1.attribute name="EECommunicationCluster.flexRayMiniSlotDuration" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="flexRayNumberOfMiniSlot" />
        <m1.attribute name="EECommunicationCluster.flexRayNumberOfMiniSlot" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="flexRayNumberOfStaticSlot" />
        <m1.attribute name="EECommunicationCluster.flexRayNumberOfStaticSlot" local="yes" />
