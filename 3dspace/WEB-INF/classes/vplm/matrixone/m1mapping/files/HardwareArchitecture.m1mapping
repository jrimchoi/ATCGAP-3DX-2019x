      </pattern.VPLM_SIMPLE>

    </pattern.VPLM_CLASS>

    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMCoreReference name="EEPhysicalChannel" />
      <m1.type name="EEPhysicalChannel" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
