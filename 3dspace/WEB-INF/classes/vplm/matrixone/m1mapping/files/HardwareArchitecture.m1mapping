      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="speed" />
        <m1.attribute name="EEPhysicalChannel.speed" local="yes" />
      </pattern.VPLM_SIMPLE>

    </pattern.VPLM_CLASS>

    <!-- Class -->
    <pattern.VPLM_CLASS>
      <vplm.PLMCoreReference name="EETopology" />
      <m1.type name="EETopology" />

    </pattern.VPLM_CLASS>

  </pattern.VPLM_PACKAGE>

</mapping>
