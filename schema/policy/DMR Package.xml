<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<policy overlay="true">
    <name>DMR Package</name>
    <symbolicName>policy_DMRPackage</symbolicName>
    <dataObjectType>policy</dataObjectType>
    <description>Policy to handle DMR Package lifecycle and access</description>
    <sequence>1,2,3,...</sequence>
    <store>STORE</store>
    <hidden>false</hidden>
    <types>
        <type>DMR Package</type>
    </types>
    <formats>
        <format>generic</format>
    </formats>
    <allState>true</allState>
    <allStateUserAccess>
        <user>
            <name>Grant</name>
            <key>GrantSpecificAccess</key>
            <filter></filter>
            <permission>read</permission>
            <permission>checkout</permission>
            <permission>show</permission>
        </user>
        <user>
            <name>public</name>
            <key></key>
            <filter></filter>
            <permission>none</permission>
        </user>
        <user>
            <name>owner</name>
            <key></key>
            <filter></filter>
            <permission>none</permission>
        </user>
    </allStateUserAccess>
    <states>
        <state>
            <stateName>In Work</stateName>
            <symbolicName>state_InWork</symbolicName>
            <versionable>true</versionable>
            <minorRevisionable>true</minorRevisionable>
            <majorRevisionable>true</majorRevisionable>
            <promote>false</promote>
            <history>true</history>
            <userAccess>
                <user>
                    <name>Quality Manager</name>
                    <key>SpecificRoleAccess</key>
                    <filter></filter>
                    <permission>read</permission>
                    <permission>modify</permission>
                    <permission>delete</permission>
                    <permission>checkout</permission>
                    <permission>checkin</permission>
                    <permission>promote</permission>
                    <permission>create</permission>
                    <permission>toconnect</permission>
                    <permission>todisconnect</permission>
                    <permission>show</permission>
                    <owner>context</owner>
                </user>
                <user>
                    <name>public</name>
                    <key></key>
                    <filter></filter>
                    <permission>none</permission>
                </user>
                <user>
                    <name>owner</name>
                    <key></key>
                    <filter></filter>
                    <permission>all</permission>
                </user>
            </userAccess>
            <triggers/>
            <signatures/>
        </state>
        <state>
            <stateName>Frozen</stateName>
            <symbolicName>state_Frozen</symbolicName>
            <versionable>true</versionable>
            <minorRevisionable>true</minorRevisionable>
            <majorRevisionable>true</majorRevisionable>
            <promote>false</promote>
            <history>true</history>
            <userAccess>
                <user>
                    <name>Quality Manager</name>
                    <key>SpecificRoleAccess</key>
                    <filter></filter>
                    <permission>read</permission>
                    <permission>checkout</permission>
                    <permission>show</permission>
                    <owner>context</owner>
                </user>
                <user>
                    <name>Global User</name>
                    <key></key>
                    <filter></filter>
                    <permission>read</permission>
                    <permission>checkout</permission>
                    <permission>show</permission>
                    <organisation>single</organisation>
                    <project>single</project>
                </user>
                <user>
                    <name>public</name>
                    <key></key>
                    <filter></filter>
                    <permission>none</permission>
                </user>
                <user>
                    <name>owner</name>
                    <key></key>
                    <filter></filter>
                    <permission>none</permission>
                </user>
            </userAccess>
            <triggers/>
            <signatures/>
        </state>
    </states>
    <properties/>
</policy>
