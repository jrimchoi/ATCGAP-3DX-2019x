<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>APPECCloseActionLink</name>
    <symbolicName>command_APPECCloseActionLink</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Close Engineering Change from Properties page</description>
    <label>emxComponents.Command.CloseEC</label>
    <hidden>false</hidden>
    <href>${ROOT_DIR}/components/emxCommonFS.jsp?functionality=ECCloseFSInstance</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>(current != "Close" &amp;&amp; current != "Reject" &amp;&amp; current != "Complete")</settingValue>
        </setting>
        <setting>
            <settingName>Access Program</settingName>
            <settingValue>emxCommonEngineeringChange</settingValue>
        </setting>
        <setting>
            <settingName>Access Program Method</settingName>
            <settingValue>showCloseToApprovalList</settingValue>
        </setting>
        <setting>
            <settingName>Popup Modal</settingName>
            <settingValue>true</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>Components</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>popup</settingValue>
        </setting>
        <setting>
            <settingName>Window Height</settingName>
            <settingValue>520</settingValue>
        </setting>
        <setting>
            <settingName>Window Width</settingName>
            <settingValue>570</settingValue>
        </setting>
    </settings>
    <userAccess/>
    <properties/>
</command>
