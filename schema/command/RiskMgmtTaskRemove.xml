<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>RiskMgmtTaskRemove</name>
    <symbolicName>command_RiskMgmtTaskRemove</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Removes the Task from the Risk</description>
    <label>RiskMgmt.Common.RemoveTask</label>
    <hidden>false</hidden>
    <href>../RiskMgmt/Execute.jsp?action=com.dassault_systemes.enovia.riskmgmt.ui.RiskMgmtRiskTasks:removeTasks&amp;suiteKey=RiskMgmt</href>
    <alt>RiskMgmt.Common.RemoveTask</alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>(current!=Controlled)</settingValue>
        </setting>
        <setting>
            <settingName>Confirm Message</settingName>
            <settingValue>RiskMgmt.Common.Remove.ConfirmMessage</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionRemove.gif</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>RiskMgmt</settingValue>
        </setting>
        <setting>
            <settingName>Row Select</settingName>
            <settingValue>multi</settingValue>
        </setting>
        <setting>
            <settingName>Submit</settingName>
            <settingValue>true</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>listHidden</settingValue>
        </setting>
    </settings>
    <userAccess/>
    <properties/>
</command>
