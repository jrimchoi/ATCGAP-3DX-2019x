<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>FTRCopyFromProductConfigurationsActionLink</name>
    <symbolicName>command_FTRCopyFromProductConfigurationsActionLink</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>CopyProductConfigurations</description>
    <label>emxProduct.ActionLink.CopyProductConfigurations</label>
    <hidden>false</hidden>
    <href>${SUITE_DIR}/CopyProductConfigurationPreProcess.jsp</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>($&lt;type.kindof=="type_Products"&gt;)&amp;&amp;(!$&lt;type=="type_ProductVariant"&gt;)&amp;&amp;(current != "Obsolete")</settingValue>
        </setting>
        <setting>
            <settingName>Hide Mode</settingName>
            <settingValue>Mobile</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionCopy.png</settingValue>
        </setting>
        <setting>
            <settingName>Licensed Product</settingName>
            <settingValue>ENO_FTR_TP,ENO_CFE_TP</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>Configuration</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>listHidden</settingValue>
        </setting>
    </settings>
    <userAccess>
        <User>Product Manager</User>
        <User>VPLMCreator</User>
    </userAccess>
    <properties/>
</command>
