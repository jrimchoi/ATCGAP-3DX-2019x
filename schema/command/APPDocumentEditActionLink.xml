<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>APPDocumentEditActionLink</name>
    <symbolicName>command_APPDocumentEditActionLink</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Document Edit Details Command Object</description>
    <label>emxComponents.Command.EditDetails</label>
    <hidden>false</hidden>
    <href>${COMMON_DIR}/emxForm.jsp?form=type_DOCUMENTS&amp;mode=edit&amp;HelpMarker=emxhelpdocumenteditdetails&amp;formHeader=emxComponents.Common.EditDetailsHeadingWithoutRev&amp;Export=False&amp;displayCDMFileSummary=true&amp;findMxLink=false</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>($&lt;attribute[attribute_SuspendVersioning].value&gt; != True &amp;&amp; current.access[modify] == true) &amp;&amp; ($&lt;attribute[attribute_IsVersionObject].value&gt; != True) &amp;&amp; (vcfile == FALSE &amp;&amp; vcfolder == FALSE) &amp;&amp; (revision==last)</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionEdit.png</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>Components</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>self</settingValue>
        </setting>
    </settings>
    <userAccess>
        <User>Global User</User>
        <User>VPLMViewer</User>
    </userAccess>
    <properties/>
</command>
