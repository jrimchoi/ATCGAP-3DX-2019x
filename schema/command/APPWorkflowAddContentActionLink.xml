<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>APPWorkflowAddContentActionLink</name>
    <symbolicName>command_APPWorkflowAddContentActionLink</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Command Object for add content to workflow object</description>
    <label>emxComponents.Common.AddContent</label>
    <hidden>false</hidden>
    <href>${COMPONENT_DIR}/emxWorkflowContentSearchDialogFS.jsp</href>
    <alt>emxComponents.Common.AddContent</alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>(current != "Suspended") &amp;&amp; (current != "Completed") &amp;&amp; (owner == context.user)</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionCreate.gif</settingValue>
        </setting>
        <setting>
            <settingName>Popup Modal</settingName>
            <settingValue>true</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>Components</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>popup</settingValue>
        </setting>
    </settings>
    <userAccess/>
    <properties/>
</command>
