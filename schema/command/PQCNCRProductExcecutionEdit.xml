<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>PQCNCRProductExcecutionEdit</name>
    <symbolicName>command_PQCNCRProductExcecutionEdit</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Edit NCR Product info in Execution State</description>
    <label>LQINCR.CommonLabel.Edit</label>
    <hidden>false</hidden>
    <href>${COMMON_DIR}/emxForm.jsp?form=PQCNCRProductControlExecutionEdit&amp;formHeader=LQINCR.EditProductControl.Execution.Header&amp;HelpMarker=NCR50&amp;mode=edit&amp;submitAction=refreshCaller</href>
    <alt>LQINCR.CommonLabel.Edit</alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>(current == "Execution") &amp;&amp;  (to[NCR ProductControl].from.current != "Invalid")</settingValue>
        </setting>
        <setting>
            <settingName>Access Function</settingName>
            <settingValue>checkNCRProductControlAccess</settingValue>
        </setting>
        <setting>
            <settingName>Access Program</settingName>
            <settingValue>iwNCRUtil</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionCreateMarkup.gif</settingValue>
        </setting>
        <setting>
            <settingName>Licensed Product</settingName>
            <settingValue>ENOCNCR_TP</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>LQINCR</settingValue>
        </setting>
        <setting>
            <settingName>Submit</settingName>
            <settingValue>true</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>popup</settingValue>
        </setting>
        <setting>
            <settingName>Window Height</settingName>
            <settingValue>580</settingValue>
        </setting>
        <setting>
            <settingName>Window Width</settingName>
            <settingValue>690</settingValue>
        </setting>
    </settings>
    <userAccess/>
    <properties/>
</command>
