        <setting>
            <settingName>Hide Mode</settingName>
            <settingValue>Mobile</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${SUITE_DIR}/images/iconReqCmdCoveringUpstream.png</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>Requirements</settingValue>
        </setting>
    </settings>
    <userAccess>
        <User>Requirement Manager</User>
        <User>VPLMCreator</User>
        <User>VPLMProjectLeader</User>
