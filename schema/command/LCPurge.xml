        <setting>
            <settingName>Confirm Message</settingName>
            <settingValue>emxLibraryCentral.Purge.ConfirmMsg</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionPurge.png</settingValue>
