<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>PQCESRPropertiesEnableEdit</name>
    <symbolicName>command_PQCESRPropertiesEnableEdit</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Displays Enable Edit command for Equipment Service Request Properties Page</description>
    <label>PQC.Common.Edit</label>
    <hidden>false</hidden>
    <href>${COMMON_DIR}/emxForm.jsp?mode=edit&amp;form=type_EquipmentServiceRequestEdit&amp;toolbar=PQCESRBasicInfoToolbar&amp;formHeader=FTComplaints.BasicInfo.Header&amp;HelpMarker=ESRPropertiesPage</href>
    <alt>FTComplaints.Complaint.PQCCPTMyComplaintViewSBEnableEdit.Label</alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>(current==Create||current=="In Process")</settingValue>
        </setting>
        <setting>
            <settingName>Access Mask</settingName>
            <settingValue>modify</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionEdit.png</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>ESRInquiry</settingValue>
        </setting>
        <setting>
            <settingName>Submit</settingName>
            <settingValue>true</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>self</settingValue>
        </setting>
    </settings>
    <userAccess/>
    <properties/>
</command>
