<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>PMCReferenceDropZone</name>
    <symbolicName>command_PMCReferenceDropZone</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Create a Drop Zone for files and Proxy</description>
    <label></label>
    <hidden>false</hidden>
    <href>${SUITE_DIR}/emxProjectManagementDragAndDrop.jsp?mode=reference</href>
    <alt>emxProgramCentral.Task.CreateDropZone</alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>( (type.kindOf[$&lt;type_BusinessGoal&gt;]==false) || (current.name != Complete) )</settingValue>
        </setting>
        <setting>
            <settingName>Access Function</settingName>
            <settingValue>hasAccessToPMCContentSummaryToolBarActions</settingValue>
        </setting>
        <setting>
            <settingName>Access Program</settingName>
            <settingValue>emxTask</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionUploadFile.png</settingValue>
        </setting>
        <setting>
            <settingName>Licensed Product</settingName>
            <settingValue>ENO_PGE_TP</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>ProgramCentral</settingValue>
        </setting>
        <setting>
            <settingName>Submit</settingName>
            <settingValue>false</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>slidein</settingValue>
        </setting>
    </settings>
    <userAccess>
        <User>Project User</User>
        <User>VPLMProjectLeader</User>
        <User>VPLMViewer</User>
    </userAccess>
    <properties/>
</command>
