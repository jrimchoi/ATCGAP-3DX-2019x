<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>APPWorkflowTaskResumeActionLink</name>
    <symbolicName>command_APPWorkflowTaskResumeActionLink</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Workflow task resume action link</description>
    <label>emxComponents.Command.Resume</label>
    <hidden>false</hidden>
    <href>${COMPONENT_DIR}/emxWorkflowTaskAction.jsp?action=resume</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>(current == "Suspended" &amp;&amp; to[Workflow Task].from.owner == context.user &amp;&amp; to[Workflow Task].from.current == "Started")</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionResume.gif</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>Components</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>formViewHidden</settingValue>
        </setting>
    </settings>
    <userAccess/>
    <properties/>
</command>
