<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>APPCDMTOVCConversionFilesActionCopy</name>
    <symbolicName>command_APPCDMTOVCConversionFilesActionCopy</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Command for converting CDM doc and copying DS file to local DS server</description>
    <label>emxComponents.VersionControl.CopyFromVC</label>
    <hidden>false</hidden>
    <href>${COMPONENT_DIR}/emxCommonDocumentPreCheckin.jsp?objectAction=convertToVCCopy&amp;actionURL=emxCommonDocumentVCConnectProcess.jsp&amp;stepOneHeader=emxComponents.Header.CopyFromVC&amp;populateDefaults=true</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>(format.hasfile == FALSE) &amp;&amp; (vcfile == FALSE &amp;&amp; vcfolder == FALSE) &amp;&amp; (current.access[checkout] == true &amp;&amp; current.access[checkin] == true)</settingValue>
        </setting>
        <setting>
            <settingName>Access Function</settingName>
            <settingValue>isCopyFromVCLinkEnabled</settingValue>
        </setting>
        <setting>
            <settingName>Access Program</settingName>
            <settingValue>emxVCDocumentUI</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMPONENT_DIR}/images/iconActionCopyFolderOrDocument.png</settingValue>
        </setting>
        <setting>
            <settingName>Popup Modal</settingName>
            <settingValue>true</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>Components</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>popup</settingValue>
        </setting>
        <setting>
            <settingName>Window Height</settingName>
            <settingValue>570</settingValue>
        </setting>
        <setting>
            <settingName>Window Width</settingName>
            <settingValue>780</settingValue>
        </setting>
    </settings>
    <userAccess>
        <User>Global User</User>
        <User>VPLMViewer</User>
    </userAccess>
    <properties/>
</command>
