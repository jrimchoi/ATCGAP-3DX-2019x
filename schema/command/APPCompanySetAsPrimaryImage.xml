<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>APPCompanySetAsPrimaryImage</name>
    <symbolicName>command_APPCompanySetAsPrimaryImage</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>APP Set As Primary Image</description>
    <label>emxComponents.Command.SetAsPrimary</label>
    <hidden>false</hidden>
    <href>${COMPONENT_DIR}/emxImageManagerSetPrimaryImage.jsp?</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>(current.access[modify] == true)</settingValue>
        </setting>
        <setting>
            <settingName>Confirm Message</settingName>
            <settingValue>emxComponents.ImageManager.Confirm.SetAsPrimary</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconSmallPrimaryImage.png</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>Components</settingValue>
        </setting>
        <setting>
            <settingName>Row Select</settingName>
            <settingValue>single</settingValue>
        </setting>
        <setting>
            <settingName>Submit</settingName>
            <settingValue>true</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>imageHidden</settingValue>
        </setting>
    </settings>
    <userAccess>
        <User>Company Representative</User>
        <User>Organization Manager</User>
        <User>VPLMAdmin</User>
    </userAccess>
    <properties/>
</command>
