<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>APPWorkflowSuspendActionLink</name>
    <symbolicName>command_APPWorkflowSuspendActionLink</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Workflow Suspend action link</description>
    <label>emxComponents.Command.Suspend</label>
    <hidden>false</hidden>
    <href>${COMPONENT_DIR}/emxWorkflowAction.jsp?action=pause</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>(current == "Started" &amp;&amp; owner == context.user)</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionPause.gif</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>Components</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>formViewHidden</settingValue>
        </setting>
    </settings>
    <userAccess/>
    <properties/>
</command>
