<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>ENCSparePartsEditAll</name>
    <symbolicName>command_ENCSparePartsEditAll</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Displays EBOM in Indented Table format</description>
    <label>emxFramework.Command.EditMaintenance</label>
    <hidden>false</hidden>
    <href>javascript:editMode()</href>
    <alt>emxFramework.Command.EditMaintenance</alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>context.user==owner &amp;&amp; (current==policy.property[state_Create].value || current==policy.property[state_PeerReview].value || current==policy.property[state_Preliminary].value)</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionEdit.png</settingValue>
        </setting>
        <setting>
            <settingName>Licensed Product</settingName>
            <settingValue>ENO_TBE_TP,ENO_PRT_TP</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>EngineeringCentral</settingValue>
        </setting>
        <setting>
            <settingName>Row Select</settingName>
            <settingValue>none</settingValue>
        </setting>
    </settings>
    <userAccess>
        <User>Design Engineer</User>
        <User>Manufacturing Engineer</User>
        <User>Senior Design Engineer</User>
        <User>Senior Manufacturing Engineer</User>
        <User>VPLMCreator</User>
        <User>VPLMProjectLeader</User>
    </userAccess>
    <properties/>
</command>
