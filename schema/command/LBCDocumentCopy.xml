<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>LBCDocumentCopy</name>
    <symbolicName>command_LBCDocumentCopy</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Copy Action Link on Properties for Document</description>
    <label>emxDocumentCentral.ActionBarCmd.Copy</label>
    <hidden>false</hidden>
    <href>${SUITE_DIR}/emxLibraryCentralIntermediateForm.jsp?mode=copy&amp;isClassifiedItem=true</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>($&lt;attribute[attribute_IsVersionObject].value&gt; != True &amp;&amp; (type=="$&lt;type_GenericDocument&gt;" || type.kindOf[$&lt;type_GenericDocument&gt;]))</settingValue>
        </setting>
        <setting>
            <settingName>Access Function</settingName>
            <settingValue>hasReadWriteAccess</settingValue>
        </setting>
        <setting>
            <settingName>Access Program</settingName>
            <settingValue>emxLibraryCentralCommon</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionCopy.gif</settingValue>
        </setting>
        <setting>
            <settingName>Popup Modal</settingName>
            <settingValue>true</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>LibraryCentral</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>slidein</settingValue>
        </setting>
    </settings>
    <userAccess>
        <User>Author</User>
        <User>Limited Author</User>
        <User>VPLMViewer</User>
    </userAccess>
    <properties/>
</command>
