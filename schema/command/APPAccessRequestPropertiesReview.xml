<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>APPAccessRequestPropertiesReview</name>
    <symbolicName>command_APPAccessRequestPropertiesReview</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Command Object to demote Access Request from Approved/Rejected state state to Review state or promote Access Request from Submitted state to Review state</description>
    <label>emxComponents.Command.ReviewAccessRequest</label>
    <hidden>false</hidden>
    <href>${SUITE_DIR}/emxCommonAccessRequestProcess.jsp?mode=Review&amp;fromPropertiesPage=true</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>( current==Submitted || current==Approved )</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>Components</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>formViewHidden</settingValue>
        </setting>
    </settings>
    <userAccess>
        <User>Request Reviewer</User>
        <User>VPLMViewer</User>
    </userAccess>
    <properties/>
</command>
