<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>APPCopyFromVC</name>
    <symbolicName>command_APPCopyFromVC</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Command for copying a file or folder from any DesignSync to local DesignSync</description>
    <label>emxComponents.VersionControl.CopyFromVC</label>
    <hidden>false</hidden>
    <href>${SUITE_DIR}/emxCommonDocumentPreCheckin.jsp?objectAction=copyFromVC&amp;actionURL=emxCommonDocumentVCConnectProcess.jsp&amp;stepOneHeader=emxComponents.Header.CopyDesignSync</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Function</settingName>
            <settingValue>isCopyFromVCLinkEnabled</settingValue>
        </setting>
        <setting>
            <settingName>Access Program</settingName>
            <settingValue>emxVCDocumentUI</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionCreate.gif</settingValue>
