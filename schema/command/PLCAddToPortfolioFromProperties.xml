<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>PLCAddToPortfolioFromProperties</name>
    <symbolicName>command_PLCAddToPortfolioFromProperties</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Adds the Product or Model in context to a Portfolio</description>
    <label>emxProduct.ActionLink.AddToPortfolio</label>
    <hidden>false</hidden>
    <href>${COMMON_DIR}/emxFullSearch.jsp?field=TYPES=type_Portfolio&amp;table=PLCSearchPortfoliosList&amp;selection=multiple&amp;submitAction=refreshCaller&amp;hideHeader=true&amp;HelpMarker=emxhelpfullsearch&amp;submitURL=../productline/SearchUtil.jsp?mode=AddExisting&amp;relName=relationship_Portfolio&amp;from=true</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>$&lt;expression[PortfolioEnabled]=="true"&gt;</settingValue>
        </setting>
        <setting>
            <settingName>Hide Mode</settingName>
            <settingValue>Mobile</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/AddToPortfolio32.png</settingValue>
        </setting>
        <setting>
            <settingName>Licensed Product</settingName>
            <settingValue>ENO_PLC_TP</settingValue>
        </setting>
        <setting>
            <settingName>Popup Modal</settingName>
            <settingValue>true</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>ProductLine</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>popup</settingValue>
        </setting>
        <setting>
            <settingName>Window Height</settingName>
            <settingValue>630</settingValue>
        </setting>
        <setting>
            <settingName>Window Width</settingName>
            <settingValue>850</settingValue>
        </setting>
    </settings>
    <userAccess/>
    <properties/>
</command>
