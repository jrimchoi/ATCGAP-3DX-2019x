<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>RMTEditSpecificationActionLink</name>
    <symbolicName>command_RMTEditSpecificationActionLink</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Opens a Edit form to update properties</description>
    <label>emxRequirements.ActionLink.Edit</label>
    <hidden>false</hidden>
    <href>${SUITE_DIR}/RMTRequirementGroupProcess.jsp?operation=editProperties</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionEdit.gif</settingValue>
        </setting>
        <setting>
            <settingName>Licensed Product</settingName>
            <settingValue>ENO_RMF_TP</settingValue>
        </setting>
        <setting>
            <settingName>Popup Modal</settingName>
            <settingValue>true</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>Requirements</settingValue>
        </setting>
        <setting>
            <settingName>Row Select</settingName>
            <settingValue>single</settingValue>
        </setting>
        <setting>
            <settingName>Submit</settingName>
            <settingValue>true</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>slidein</settingValue>
        </setting>
        <setting>
            <settingName>Window Height</settingName>
            <settingValue>520</settingValue>
        </setting>
        <setting>
            <settingName>Window Width</settingName>
            <settingValue>570</settingValue>
        </setting>
    </settings>
    <userAccess>
        <User>Requirement Manager</User>
        <User>VPLMCreator</User>
        <User>VPLMProjectAdministrator</User>
    </userAccess>
    <properties/>
</command>
