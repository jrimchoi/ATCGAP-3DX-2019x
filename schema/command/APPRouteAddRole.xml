<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>APPRouteAddRole</name>
    <symbolicName>command_APPRouteAddRole</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Command to add roles to the route access</description>
    <label>emxComponents.AddMembersDialog.AddRole</label>
    <hidden>false</hidden>
    <href>${SUITE_DIR}/emxRouteMembersSearch.jsp?fromPage=RouteAccessSummary&amp;memberType=Role</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>(to[Route Scope] == False)</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionAdd.png</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>Components</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>listHidden</settingValue>
        </setting>
    </settings>
    <userAccess>
        <User>Global User</User>
        <User>VPLMViewer</User>
    </userAccess>
    <properties/>
</command>
