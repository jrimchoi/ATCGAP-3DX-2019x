<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>APPWorkflowTaskAcceptActionLink</name>
    <symbolicName>command_APPWorkflowTaskAcceptActionLink</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Workflow task accept action link</description>
    <label>emxComponents.Command.Accept</label>
    <hidden>false</hidden>
    <href>${COMPONENT_DIR}/emxWorkflowTaskAction.jsp?action=accept</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>(current=="Started") &amp;&amp; ((owner.isaperson != true &amp;&amp; owner==context.user.assignment) || (owner.isaperson==true &amp;&amp; owner==context.user.name))</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>Components</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>formViewHidden</settingValue>
        </setting>
    </settings>
    <userAccess/>
    <properties/>
</command>
