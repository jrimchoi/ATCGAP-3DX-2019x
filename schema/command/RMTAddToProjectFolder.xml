<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>RMTAddToProjectFolder</name>
    <symbolicName>command_RMTAddToProjectFolder</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Opens a Project search results and the folder tree is displayed to add the Specifications.</description>
    <label>emxRequirements.ActionLink.AddtoProjectFolder</label>
    <hidden>false</hidden>
    <href>${SUITE_DIR}/SpecificationProcess.jsp?rmtInstall=true&amp;mode=addToProject&amp;HelpMarker=emxhelpprojectsearchresults</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Function</settingName>
            <settingValue>isPRGInstalled</settingValue>
        </setting>
        <setting>
            <settingName>Access Program</settingName>
            <settingValue>emxRMTCommon</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionAddToProjectFolder.gif</settingValue>
        </setting>
        <setting>
            <settingName>Licensed Product</settingName>
            <settingValue>ENO_RMF_TP</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>Requirements</settingValue>
        </setting>
        <setting>
            <settingName>Row Select</settingName>
            <settingValue>multi</settingValue>
        </setting>
        <setting>
            <settingName>Submit</settingName>
            <settingValue>true</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>hidden</settingValue>
        </setting>
    </settings>
    <userAccess>
        <User>Requirement Manager</User>
        <User>VPLMCreator</User>
        <User>VPLMProjectAdministrator</User>
    </userAccess>
    <properties/>
</command>
