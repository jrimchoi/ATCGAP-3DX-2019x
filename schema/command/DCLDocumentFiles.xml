<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>DCLDocumentFiles</name>
    <symbolicName>command_DCLDocumentFiles</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Lists the Files in the Document Object</description>
    <label>emxComponents.Common.Files</label>
    <hidden>false</hidden>
    <href>${COMMON_DIR}/emxIndentedTable.jsp?program=emxCommonFileUI:getFiles&amp;table=DOCOFileSummary&amp;selection=multiple&amp;toolbar=DCLFileSummaryToolBar&amp;header=emxComponents.Menu.Files&amp;subHeader=emxComponents.Menu.SubHeaderDocuments&amp;HelpMarker=emxhelpdocumentfilelist&amp;FilterFramePage=../components/emxCommonDocumentCheckoutUtil.jsp&amp;FilterFrameSize=1&amp;showClipboard=false&amp;sortDirection=ascending&amp;sortColumnName=FileSequence&amp;hideRootSelection=true</href>
    <alt>emxComponents.Common.Files</alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>($&lt;attribute[attribute_IsVersionObject].value&gt; != True) &amp;&amp; (vcfile == FALSE &amp;&amp; vcfolder == FALSE &amp;&amp; vcmodule == FALSE)</settingValue>
        </setting>
        <setting>
            <settingName>Access Function</settingName>
            <settingValue>allowFileVersioning</settingValue>
        </setting>
        <setting>
            <settingName>Access Program</settingName>
            <settingValue>emxCommonDocument</settingValue>
        </setting>
        <setting>
            <settingName>Licensed Product</settingName>
            <settingValue>ENODOCO_TP</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>Components</settingValue>
        </setting>
    </settings>
    <userAccess/>
    <properties/>
</command>
