<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>APPWorkflowTaskOverrideActionLink</name>
    <symbolicName>command_APPWorkflowTaskOverrideActionLink</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Workflow task override action link</description>
    <label>emxComponents.Command.Override</label>
    <hidden>false</hidden>
    <href>${COMPONENT_DIR}/emxWorkflowTaskAction.jsp?action=override</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>(current == "Started" || current == "Assigned") &amp;&amp; to[Workflow Task].from.owner == context.user &amp;&amp; to[Workflow Task].from.current == "Started"</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>Components</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>formViewHidden</settingValue>
        </setting>
    </settings>
    <userAccess/>
    <properties/>
</command>
