<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>QICAUDAuditlLifecycleDemote</name>
    <symbolicName>command_QICAUDAuditlLifecycleDemote</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Audit Lifecycle Toolbar - Demote Audit</description>
    <label>LQIAudit.Button.Demote</label>
    <hidden>false</hidden>
    <href>javascript:lockDemote('emxLifecycleProcess.jsp?cmd=demote')\;</href>
    <alt>LQIAudit.Button.Demote</alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>((current != Plan) &amp;&amp; (current != Closed) &amp;&amp; (context.user.assignment == "$&lt;role_AuditAdmin&gt;" || context.user == owner || context.user.assignment == "$&lt;role_AuditLead&gt;"))</settingValue>
        </setting>
        <setting>
            <settingName>Access Mask</settingName>
            <settingValue>Demote</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionDemote.gif</settingValue>
        </setting>
        <setting>
            <settingName>Licensed Product</settingName>
            <settingValue>ENO_QIC_TP</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>LQIAudit</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>hiddenFrame</settingValue>
        </setting>
    </settings>
    <userAccess>
        <User>Employee</User>
    </userAccess>
    <properties/>
</command>
