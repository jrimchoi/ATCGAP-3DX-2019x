<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>APPAccessRequestPropertiesApprove</name>
    <symbolicName>command_APPAccessRequestPropertiesApprove</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Command Object to promote Access Request from Submitted/Review state state to Approved state</description>
    <label>emxComponents.Command.ApproveAccessRequest</label>
    <hidden>false</hidden>
    <href>${SUITE_DIR}/emxCommonAccessRequestProcess.jsp?mode=Approved&amp;fromPropertiesPage=true</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>( current==Submitted || current==Review )</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionApprove.png</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>Components</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>formViewHidden</settingValue>
        </setting>
    </settings>
    <userAccess>
        <User>Request Reviewer</User>
        <User>VPLMViewer</User>
    </userAccess>
    <properties/>
</command>
