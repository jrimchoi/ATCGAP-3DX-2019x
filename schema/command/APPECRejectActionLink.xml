<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>APPECRejectActionLink</name>
    <symbolicName>command_APPECRejectActionLink</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Reject Engineering Change from Properties page</description>
    <label>emxComponents.Command.RejectEC</label>
    <hidden>false</hidden>
    <href>${ROOT_DIR}/components/emxCommonFS.jsp?functionality=ECRejectFSInstance</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>(current != "Close" &amp;&amp; current != "Complete" &amp;&amp; current != "Reject")</settingValue>
        </setting>
        <setting>
            <settingName>Access Program</settingName>
            <settingValue>emxCommonEngineeringChange</settingValue>
        </setting>
        <setting>
            <settingName>Access Program Method</settingName>
            <settingValue>showRejectToApprovalList</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionReject.png</settingValue>
        </setting>
        <setting>
            <settingName>Popup Modal</settingName>
            <settingValue>true</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>Components</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>popup</settingValue>
        </setting>
        <setting>
            <settingName>Window Height</settingName>
            <settingValue>520</settingValue>
        </setting>
        <setting>
            <settingName>Window Width</settingName>
            <settingValue>570</settingValue>
        </setting>
    </settings>
    <userAccess/>
    <properties/>
</command>
