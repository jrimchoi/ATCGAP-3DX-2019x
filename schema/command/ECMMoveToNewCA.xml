<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>ECMMoveToNewCA</name>
    <symbolicName>command_ECMMoveToNewCA</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>The Move to New CA Action Link on the CO Affected Items Page</description>
    <label>EnterpriseChangeMgt.Command.NewCA</label>
    <hidden>false</hidden>
    <href>${SUITE_DIR}/ECMFullSearchPostProcess.jsp?functionality=MoveToNewCA</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>(current==policy.property[state_Propose].value || current==policy.property[state_Prepare].value || current==policy.property[state_Create].value || current==policy.property[state_Evaluate].value) &amp;&amp; (owner==context.user)</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionCreate.png</settingValue>
        </setting>
        <setting>
            <settingName>Licensed Product</settingName>
            <settingValue>ENO_ECM_TP</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>EnterpriseChangeMgt</settingValue>
        </setting>
        <setting>
            <settingName>Row Select</settingName>
            <settingValue>Multiple</settingValue>
        </setting>
        <setting>
            <settingName>Submit</settingName>
            <settingValue>true</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>listHidden</settingValue>
        </setting>
    </settings>
    <userAccess/>
    <properties/>
</command>
