<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>FTRRecalculatePriceAction</name>
    <symbolicName>command_FTRRecalculatePriceAction</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Command is used to Recalculate Price of Out Of Date PCs.</description>
    <label>emxProduct.ActionLink.RecalculatePrice</label>
    <hidden>false</hidden>
    <href>${SUITE_DIR}/ProductConfigurationRecomputeListPrice.jsp?mode=RecalculateOnSummaryAction</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>$&lt;type.kindof == "type_Products"&gt; &amp;&amp; $&lt;type!="type_ProductVariant"&gt;</settingValue>
        </setting>
        <setting>
            <settingName>Hide Mode</settingName>
            <settingValue>Mobile</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionRecalculatePrice.png</settingValue>
        </setting>
        <setting>
            <settingName>Licensed Product</settingName>
            <settingValue>ENO_FTR_TP,ENO_CFE_TP</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>Configuration</settingValue>
        </setting>
        <setting>
            <settingName>Row Select</settingName>
            <settingValue>multiple</settingValue>
        </setting>
        <setting>
            <settingName>Submit</settingName>
            <settingValue>true</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>listHidden</settingValue>
        </setting>
    </settings>
    <userAccess/>
    <properties/>
</command>
