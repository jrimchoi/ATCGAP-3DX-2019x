<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>ECMCAEnableEdit</name>
    <symbolicName>command_ECMCAEnableEdit</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description></description>
    <label>emxFramework.TableEdit.Enable_Edit</label>
    <hidden>false</hidden>
    <href>javascript:editMode()</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>(current==policy.property[state_Prepare].value || current==policy.property[state_InWork].value) &amp;&amp; (owner==context.user)</settingValue>
