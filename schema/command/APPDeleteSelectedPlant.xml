<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>APPDeleteSelectedPlant</name>
    <symbolicName>command_APPDeleteSelectedPlant</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>This command is used to delete the selected Plant Objects associated with a Company</description>
    <label>emxComponents.Command.DeleteSelectedPlant</label>
    <hidden>false</hidden>
    <href>${SUITE_DIR}/emxComponentsDeletePlant.jsp</href>
    <alt></alt>
    <code></code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>(current.access[delete] == true)</settingValue>
        </setting>
        <setting>
            <settingName>Confirm Message</settingName>
            <settingValue>emxComponents.PlantSummary.MsgConfirm</settingValue>
        </setting>
        <setting>
            <settingName>Image</settingName>
            <settingValue>${COMMON_DIR}/images/iconActionDelete.png</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>Components</settingValue>
        </setting>
        <setting>
            <settingName>Row Select</settingName>
            <settingValue>multiple</settingValue>
        </setting>
        <setting>
            <settingName>Submit</settingName>
            <settingValue>true</settingValue>
        </setting>
        <setting>
            <settingName>Target Location</settingName>
            <settingValue>listHidden</settingValue>
        </setting>
    </settings>
    <userAccess>
        <User>Company Representative</User>
        <User>Organization Manager</User>
        <User>VPLMAdmin</User>
    </userAccess>
    <properties/>
</command>
