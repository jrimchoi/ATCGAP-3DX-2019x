<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<command overlay="true">
    <name>QICCPRFilter</name>
    <symbolicName>command_QICCPRFilter</symbolicName>
    <dataObjectType>command</dataObjectType>
    <description>Command to show Assigned/MyOpened/MyCosed filter to Users other than Quality Manger</description>
    <label></label>
    <hidden>false</hidden>
    <href></href>
    <alt></alt>
    <code>commandSource\QICCPRFilter</code>
    <settings>
        <setting>
            <settingName>Access Expression</settingName>
            <settingValue>!(context.user.isassigned[$&lt;role_QualityManager&gt;]==true || context.user.isassigned[$&lt;role_VPLMProjectLeader&gt;]==true)</settingValue>
        </setting>
        <setting>
            <settingName>Default</settingName>
            <settingValue>Assigned</settingValue>
        </setting>
        <setting>
            <settingName>Input Type</settingName>
            <settingValue>combobox</settingValue>
        </setting>
        <setting>
            <settingName>Licensed Product</settingName>
            <settingValue>ENO_QIC_TP</settingValue>
        </setting>
        <setting>
            <settingName>Range Display Values</settingName>
            <settingValue>QIC.CAPARequest.Filter.Assigned.Label,QIC.CAPARequest.Filter.Opened.Label,QIC.CAPARequest.Filter.Closed.Label</settingValue>
        </setting>
        <setting>
            <settingName>Range Values</settingName>
            <settingValue>Assigned,Opened,Closed</settingValue>
        </setting>
        <setting>
            <settingName>Registered Suite</settingName>
            <settingValue>LQICAPA</settingValue>
        </setting>
    </settings>
    <userAccess/>
    <properties/>
</command>
