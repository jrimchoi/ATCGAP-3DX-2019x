<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<type overlay="true">
    <name>Software Feature</name>
    <symbolicName>type_SoftwareFeature</symbolicName>
    <dataObjectType>type</dataObjectType>
    <description>represents a feature in a software, there can be many features getting implemented in a software</description>
    <parentName>Logical Feature</parentName>
    <hidden>false</hidden>
    <abstract>false</abstract>
    <attributes/>
    <triggers>
        <trigger immutable="true">
            <triggerType>Create</triggerType>
            <programName>emxTriggerManager</programName>
            <input>TypeAllCreateAction TypeSoftwareFeatureCreateAction</input>
            <programType>Action</programType>
        </trigger>
        <trigger immutable="true">
            <triggerType>Revision</triggerType>
            <programName>emxTriggerManager</programName>
            <input>TypeLogicalFeatureReviseAction</input>
            <programType>Action</programType>
        </trigger>
    </triggers>
    <properties/>
</type>
