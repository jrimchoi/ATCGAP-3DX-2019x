<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<type overlay="true">
    <name>Weekly Timesheet</name>
    <symbolicName>type_WeeklyTimesheet</symbolicName>
    <dataObjectType>type</dataObjectType>
    <description>Defines a Weekly Timesheet which holds individuals Efforts</description>
    <parentName></parentName>
    <hidden>false</hidden>
    <abstract>false</abstract>
    <attributes>
        <attribute>Approved Effort</attribute>
        <attribute>Approver Comments</attribute>
        <attribute>Remaining Effort</attribute>
        <attribute>User Comments</attribute>
        <attribute>Week Ending Date</attribute>
    </attributes>
    <triggers>
        <trigger immutable="true">
            <triggerType>Delete</triggerType>
            <programName>emxTriggerManager</programName>
            <input>TypeWeeklyTimesheetDeleteCheck</input>
            <programType>Check</programType>
        </trigger>
        <trigger immutable="true">
            <triggerType>Delete</triggerType>
            <programName>emxTriggerManager</programName>
            <input>TypeWeeklyTimesheetDeleteAction</input>
            <programType>Action</programType>
        </trigger>
    </triggers>
    <properties/>
</type>
