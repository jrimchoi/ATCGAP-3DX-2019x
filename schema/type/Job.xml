<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<type overlay="true">
    <name>Job</name>
    <symbolicName>type_Job</symbolicName>
    <dataObjectType>type</dataObjectType>
    <description>Object that gets created every time a background job is submitted also to hold input/output/log files during Background Process</description>
    <parentName></parentName>
    <hidden>false</hidden>
    <abstract>false</abstract>
    <attributes>
        <attribute>Aborted Requested</attribute>
        <attribute>Action On Completion</attribute>
        <attribute>Allow Reexecute</attribute>
        <attribute>Auto Start On Startup</attribute>
        <attribute>Completion Status</attribute>
        <attribute>Current Activity</attribute>
        <attribute>Error Message</attribute>
        <attribute>Method Name</attribute>
        <attribute>Next Step Command</attribute>
        <attribute>Notify Owner</attribute>
        <attribute>Originator</attribute>
        <attribute>Program Arguments</attribute>
        <attribute>Program Name</attribute>
        <attribute>Progress Percent</attribute>
        <attribute>Restart Point</attribute>
        <attribute>Server Unique Identifier</attribute>
        <attribute>Start Date</attribute>
        <attribute>Title</attribute>
    </attributes>
    <triggers>
        <trigger immutable="true">
            <triggerType>Create</triggerType>
            <programName>emxTriggerManager</programName>
            <input>TypeAllCreateAction</input>
            <programType>Action</programType>
        </trigger>
        <trigger immutable="true">
            <triggerType>Revision</triggerType>
            <programName>emxTriggerManager</programName>
            <input>TypeAllReviseAction</input>
            <programType>Action</programType>
        </trigger>
    </triggers>
    <properties/>
</type>
