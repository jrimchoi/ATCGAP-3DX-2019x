<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<type overlay="true">
    <name>Media</name>
    <symbolicName>type_Media</symbolicName>
    <dataObjectType>type</dataObjectType>
    <description>Object that will be created by a Quality Engineer for storage of the Software Distribution Object(which can be tape,cdrom etc.,)</description>
    <parentName></parentName>
    <hidden>false</hidden>
    <abstract>false</abstract>
    <attributes>
        <attribute>Label</attribute>
        <attribute>Originator</attribute>
        <attribute>Unit Price</attribute>
    </attributes>
    <triggers>
        <trigger immutable="true">
            <triggerType>Create</triggerType>
            <programName>emxTriggerManager</programName>
            <input>TypeAllCreateAction TypeMediaCreateAction</input>
            <programType>Action</programType>
        </trigger>
        <trigger immutable="true">
            <triggerType>Revision</triggerType>
            <programName>emxTriggerManager</programName>
            <input>TypeAllReviseAction TypeMediaReviseAction</input>
            <programType>Action</programType>
        </trigger>
    </triggers>
    <properties/>
</type>
