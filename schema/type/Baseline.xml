<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<type overlay="true">
    <name>Baseline</name>
    <symbolicName>type_Baseline</symbolicName>
    <dataObjectType>type</dataObjectType>
    <description>the baseline of the software release at a particular point of time in its development cycle</description>
    <parentName></parentName>
    <hidden>false</hidden>
    <abstract>false</abstract>
    <attributes>
        <attribute>Baseline Text</attribute>
        <attribute>Notes</attribute>
        <attribute>Originator</attribute>
        <attribute>Synopsis</attribute>
    </attributes>
    <triggers>
        <trigger immutable="true">
            <triggerType>Create</triggerType>
            <programName>emxTriggerManager</programName>
            <input>TypeAllCreateAction TypeBaselineCreateAction</input>
            <programType>Action</programType>
        </trigger>
        <trigger immutable="true">
            <triggerType>Revision</triggerType>
            <programName>emxTriggerManager</programName>
            <input>TypeAllReviseAction TypeBaselineReviseAction</input>
            <programType>Action</programType>
        </trigger>
    </triggers>
    <properties/>
</type>
