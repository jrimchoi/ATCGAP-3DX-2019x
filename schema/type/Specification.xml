<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<type overlay="true">
    <name>Specification</name>
    <symbolicName>type_Specification</symbolicName>
    <dataObjectType>type</dataObjectType>
    <description>the abstract Specification type, which serves as the base type for funtional specification, design specification and test specification</description>
    <parentName>DOCUMENTS</parentName>
    <hidden>false</hidden>
    <abstract>true</abstract>
    <attributes>
        <attribute>File Version</attribute>
        <attribute>Notes</attribute>
        <attribute>Objective</attribute>
        <attribute>Synopsis</attribute>
        <attribute>Version</attribute>
        <attribute>Version Date</attribute>
    </attributes>
    <triggers>
        <trigger immutable="true">
            <triggerType>Create</triggerType>
            <programName>emxTriggerManager</programName>
            <input>TypeAllCreateAction TypeSpecificationCreateAction</input>
            <programType>Action</programType>
        </trigger>
        <trigger immutable="true">
            <triggerType>Revision</triggerType>
            <programName>emxTriggerManager</programName>
            <input>TypeAllReviseAction TypeDOCUMENTSReviseAction TypeSpecificationReviseAction</input>
            <programType>Action</programType>
        </trigger>
    </triggers>
    <properties/>
</type>
