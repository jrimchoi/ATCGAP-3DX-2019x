<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<type overlay="true">
    <name>Application Part</name>
    <symbolicName>type_ApplicationPart</symbolicName>
    <dataObjectType>type</dataObjectType>
    <description>This type is used as an intermediate object when Application Context MEP is created</description>
    <parentName></parentName>
    <hidden>false</hidden>
    <abstract>false</abstract>
    <attributes>
        <attribute>Effectivity Date</attribute>
        <attribute>Estimated Cost</attribute>
        <attribute>Lead Time Duration</attribute>
        <attribute>Material Category</attribute>
        <attribute>Originator</attribute>
        <attribute>Part Classification</attribute>
        <attribute>Production Make Buy Code</attribute>
        <attribute>Service Make Buy Code</attribute>
        <attribute>Target Cost</attribute>
        <attribute>Unit of Measure</attribute>
        <attribute>Weight</attribute>
    </attributes>
    <triggers>
        <trigger immutable="true">
            <triggerType>Create</triggerType>
            <programName>emxTriggerManager</programName>
            <input>TypeAllCreateAction TypeApplicationPartCreateAction</input>
            <programType>Action</programType>
        </trigger>
        <trigger immutable="true">
            <triggerType>Revision</triggerType>
            <programName>emxTriggerManager</programName>
            <input>TypeAllReviseAction TypeApplicationPartReviseAction</input>
            <programType>Action</programType>
        </trigger>
    </triggers>
    <properties/>
</type>
