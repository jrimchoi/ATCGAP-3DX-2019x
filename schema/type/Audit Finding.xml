<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<type overlay="true">
    <name>Audit Finding</name>
    <symbolicName>type_AuditFinding</symbolicName>
    <dataObjectType>type</dataObjectType>
    <description>LQIAudit - Object to track audit findings</description>
    <parentName></parentName>
    <hidden>false</hidden>
    <abstract>false</abstract>
    <attributes>
        <attribute>Audit Auditor</attribute>
        <attribute>Audit Date Verified</attribute>
        <attribute>Audit External Reference</attribute>
        <attribute>Audit Finding Response</attribute>
        <attribute>Audit Follow-Up Required</attribute>
        <attribute>Audit Follow-Up Through Re-Audit</attribute>
        <attribute>Audit Functional Area</attribute>
        <attribute>Audit Include Finding in Final Report</attribute>
        <attribute>Audit No Follow-Up Rationale</attribute>
        <attribute>Audit Planned Follow-Up Date</attribute>
        <attribute>Audit Previous Finding Reference</attribute>
        <attribute>Audit Probability of Occurrence</attribute>
        <attribute>Audit ReAudit</attribute>
        <attribute>Audit Recommendation</attribute>
        <attribute>Audit Regulatory Requirements</attribute>
        <attribute>Audit Repeat Finding</attribute>
        <attribute>Audit Resolution Assigned To</attribute>
        <attribute>Audit Resolution Summary</attribute>
        <attribute>Audit Responder</attribute>
        <attribute>Audit Response Type</attribute>
        <attribute>Audit Severity</attribute>
        <attribute>Audit Sub-System</attribute>
        <attribute>Audit Verified By</attribute>
    </attributes>
    <triggers>
        <trigger immutable="true">
            <triggerType>Revision</triggerType>
            <programName>emxTriggerManager</programName>
            <input>TypeAuditFindingRevisionAction</input>
            <programType>Action</programType>
        </trigger>
        <trigger immutable="true">
            <triggerType>ChangeOwner</triggerType>
            <programName>emxTriggerManager</programName>
            <input>TypeAuditFindingChangeOwnerOverride</input>
            <programType>Override</programType>
        </trigger>
    </triggers>
    <properties/>
</type>
